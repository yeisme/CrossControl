{
    "version": 8,
    "configurePresets": [
        {
            "name": "msvc2022-debug",
            "displayName": "Microsoft Visual Studio 2022 Debug",
            "description": "Debug build configuration for Desktop Qt 6.8.3 msvc2022 64-bit",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build/Desktop_Qt_6_8_3_msvc2022_64_bit-Debug",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_COLOR_DIAGNOSTICS": "ON",
                "CMAKE_PREFIX_PATH": "C:/Qt/6.8.3/msvc2022_64/",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
                "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
                "CMAKE_C_COMPILER": "cl.exe",
                "CMAKE_CXX_COMPILER": "cl.exe"
            },
            "warnings": {
                "dev": false
            },
            "condition": {
                "type": "equals",
                "lhs": "${hostSystemName}",
                "rhs": "Windows"
            }
        },
        {
            "name": "msvc2022-release",
            "displayName": "Microsoft Visual Studio 2022 Release",
            "description": "Release build configuration for Desktop Qt 6.8.3 msvc2022 64-bit",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build/Desktop_Qt_6_8_3_msvc2022_64_bit-Release",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "CMAKE_COLOR_DIAGNOSTICS": "ON",
                "CMAKE_PREFIX_PATH": "C:/Qt/6.8.3/msvc2022_64/",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
                "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
                "CMAKE_C_COMPILER": "cl.exe",
                "CMAKE_CXX_COMPILER": "cl.exe"
            },
            "warnings": {
                "dev": false
            },
            "condition": {
                "type": "equals",
                "lhs": "${hostSystemName}",
                "rhs": "Windows"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "msvc2022-debug-build",
            "configurePreset": "msvc2022-debug",
            "targets": [
                "all"
            ]
        },
        {
            "name": "msvc2022-release-build",
            "configurePreset": "msvc2022-release",
            "targets": [
                "all"
            ]
        }
    ],
    "packagePresets": [
        {
            "name": "msvc2022-release",
            "configurePreset": "msvc2022-release",
            "packageName": "CrossControl",
            "packageDirectory": "../msvc2022-release",
            "generators": [
                "7Z",
                "INNOSETUP"
            ],
            "condition": {
                "type": "equals",
                "lhs": "${hostSystemName}",
                "rhs": "Windows"
            }
        }
    ],
    "workflowPresets": [
        {
            "name": "msvc2022-release-workflow",
            "description": "",
            "displayName": "",
            "steps": [
                {
                    "type": "configure",
                    "name": "msvc2022-release"
                },
                {
                    "type": "build",
                    "name": "msvc2022-release-build"
                },
                {
                    "type": "package",
                    "name": "msvc2022-release"
                }
            ]
        }
    ]
}